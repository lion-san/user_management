<h1>Logs#index</h1>
<p>Find me in app/views/logs/index.html.erb</p>

<div  class="container">
<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Time</th>
      <th>Userid</th>
      <th>Action</th>
      <th>Var1</th>
      <th>Var2</th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.time %></td>
        <td><%= log.userid %></td>
        <td><%= log.action %></td>
        <td><%= log.val1 %></td>
        <td><%= log.val2 %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

